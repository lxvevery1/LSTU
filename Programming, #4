#include <stdio.h> 
#include <math.h>
int main()
{
    int x, y, a, b;           // Ввводим переменные

    x = 6;                    // Переменная 1 

    y = 10;                   // Переменная 2 

    a = (x + y) / 2.0;        // Среднее арифметическое переменных x и y

    b = sqrt(x * y);          // Cреднее геометрическое переменных x и y 

    printf("%d\n", a);     // Вывод переменной "a" на экран
    printf("%d\n", b);     // Вывод переменной "b" на экран 
return 0;
}
