@startuml
' Определение сущностей
entity Пользователь {
  + Логин : string
  --
  Пароль : string
  Роль : string
  E-mail : string
  Телефон : string
}

entity МедНабор {
  + ID_МедНабора : int
  --
  Номер : int
  Название : string
  Подразделение : string
  Комментарий : string
  Состояние : string
  Место_Хранения : string
}

entity Объект {
  + ID_Объекта : int
  --
  Название : string
  Адрес : string
  Тип : string
}

entity ЗапросМедНабора {
  + ID_Запроса : int
  --
  Дата : date
}

entity ЖурналПеремещений {
  + ID_Журнала : int
  --
  Дата : date
}

' Определение связей
Пользователь ||--o{ ЗапросМедНабора : "создает"
Пользователь ||--o{ ЗапросМедНабора : "принимает"
Пользователь }o--o{ МедНабор : "редактирует"
Пользователь }o--o{ Объект : "редактирует"
МедНабор |o--|| ЗапросМедНабора : "запрашивается"
Объект |o--o{ МедНабор : "отправляет"
Объект |o--o{ МедНабор : "получает"
ЖурналПеремещений ||--|| МедНабор : "добавляет"

@enduml
