@startuml
' Определение сущностей
object Пользователь {
  + Логин : string (обяз.)
  --
  Пароль : string (обяз.)
  Роль : string (обяз.)
  E-mail : string (не обяз.)
  Телефон : string (не обяз.)
}
object МедНабор {
  + ID_МедНабор : int (обяз.)
  --
  Номер : int (обяз.)
  Название : string (обяз.)
  Подразделение : string (обяз.)
  Комментарий : string (не обяз.)
  Состояние : string (обяз.)
  _ID_Объект : int (обяз.)
}
object Объект {
  + ID_Объект : int (обяз.)
  --
  Название : string (обяз.)
  Адрес : string (обяз.)
  Тип : string (обяз.)

  _ID_Хранящийся_набор : int[] (необяз.)
}
object ЗапросМедНабора {
  + ID_Запрос : int (обяз.)
  --
  Дата : date (обяз.)
  Принят : bool (обяз.)

  _Логин_отправителя : string (обяз.)
  _Логин_получателя : string (обяз.)

  _ID_МедНабор : int (обяз.)
}
object ЖурналПеремещений {
  + ID_Журнал : int (обяз.)
  --
  Дата : date (обяз.)

  _ID_МедНабор : int (обяз.)
  _ID_Объект_Отправитель : int (обяз.)
  _ID_Объект_Получатель : int (обяз.)
}

' Определение связей
Пользователь <-- ЗапросМедНабора : "создает"
Пользователь <-- ЗапросМедНабора : "принимает"
МедНабор <-- ЗапросМедНабора : "запрашивается"
Объект <-- МедНабор : "отправляется"
Объект --> МедНабор : "хранит"
ЖурналПеремещений --> МедНабор : "добавляет_набор"
ЖурналПеремещений --> Объект : "получает_набор"


@enduml
